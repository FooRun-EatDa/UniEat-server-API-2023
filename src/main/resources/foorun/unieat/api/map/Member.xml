<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="${base-namespace}.memberBase">
    <resultMap id="member" type="UniEatMember">
        <id property="pId" column="member_id" javaType="Long" />
        <result property="email" column="member_email" javaType="String" />
        <result property="password" column="member_password" javaType="String" />
        <result property="lastSignInAt" column="last_sign_in_at" javaType="LocalDateTime" />
        <result property="status" column="manage_status" typeHandler="foorun.unieat.api.config.mybatis.handler.ManagedStatusTypeHandler" />
        <result property="role" column="member_role" typeHandler="foorun.unieat.api.config.mybatis.handler.MemberRoleHandler" />
        <result property="lockedDate" column="locked_date" javaType="LocalDateTime" />
        <result property="expiredDate" column="expired_date" javaType="LocalDateTime" />
        <result property="createdDate" column="created_at" javaType="LocalDateTime" />
        <result property="modifiedDate" column="updated_at" javaType="LocalDateTime" />
    </resultMap>
</mapper>